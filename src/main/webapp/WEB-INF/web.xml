<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UpdateCustomerController</servlet-name>
  	<display-name>UpdateCustomerController</display-name>
  	<description></description>
  	<servlet-class>com.jsp.Controller.UpdateCustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteCustomerController</servlet-name>
  	<display-name>DeleteCustomerController</display-name>
  	<description></description>
  	<servlet-class>com.jsp.Controller.DeleteCustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginUserController</servlet-name>
  	<display-name>LoginUserController</display-name>
  	<description></description>
  	<servlet-class>com.jsp.Controller.LoginUser</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UpdateCustomerController</servlet-name>
  	<url-pattern>/UpdateCustomerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteCustomerController</servlet-name>
  	<url-pattern>/DeleteCustomerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginUserController</servlet-name>
  	<url-pattern>/LoginUserController</url-pattern>
  </servlet-mapping>
</web-app>
